{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "[%(asctime)s.%(msecs)03d] [external-server] [%(levelname)s]\t %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
     "formatter": "simple",
       "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "external_server.server": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ],
      "propagate": false
    },
    "external_server.adapters.mqtt_client": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ],
      "propagate": false
    },
    "tests.utils._mqtt_broker_test": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ],
      "propagate": false
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console"
    ]
  }
}